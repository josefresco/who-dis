{
  "manifest_version": 3,
  "name": "IP Lookup - Who Dis?",
  "version": "1.0",
  "description": "Right-click on any IP address to look it up using a third-party service",
  "permissions": [
    "contextMenus",
    "activeTab",
    "notifications"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}